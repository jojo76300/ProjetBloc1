<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="wwe.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:wwe.Pages"
    Title="wwe">

    <TabBar>
        <Tab Icon="home.png">
            <ShellContent
                ContentTemplate="{DataTemplate local:MainPage}"
                Route="MainPage" />
        </Tab>

        <!--<Tab Icon="accueil.png">
            <ShellContent
                ContentTemplate="{DataTemplate local:Accueil}"
                Route="Accueil" />
        </Tab>-->

        <Tab Icon="catcheur.png">
            <ShellContent
                ContentTemplate="{DataTemplate local:Catcheur}"
                Route="Catcheur" />
        </Tab>

        <Tab Icon="championnat.png">
            <ShellContent
                ContentTemplate="{DataTemplate local:Championnat}"
                Route="Championnat" />
        </Tab>
    </TabBar>

</Shell>
