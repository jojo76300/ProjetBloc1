<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="wwe.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:wwe"
    Title="wwe">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Accueil"
        ContentTemplate="{DataTemplate local:Pages.Accueil}"
        Route="Accueil" />
    
    <ShellContent
        Title="Catcheurs"
        ContentTemplate="{DataTemplate local:Pages.Catcheur}"
        Route="Catcheur" />

    <ShellContent
        Title="Championnats"
        ContentTemplate="{DataTemplate local:Pages.Championnat}"
        Route="Championnat" />
    
</Shell>
