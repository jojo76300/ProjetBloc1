<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="wwe.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:wwe.Pages"
    Title="wwe">
<TabBar>
    <Tab Icon="accueil.png">
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" /> 
    </Tab>
</TabBar> 
    
<TabBar>
    <Tab>
    <ShellContent
        Title="Accueil"
        ContentTemplate="{DataTemplate local:Accueil}"
        Route="Accueil" />
    </Tab>
</TabBar>  
    
    <TabBar>
        <Tab>
        <ShellContent
            Title="Catcheurs"
            ContentTemplate="{DataTemplate local:Catcheur}"
            Route="Catcheur" />
        </Tab>
    
    </TabBar>
    
    <TabBar>
        <Tab>
        <ShellContent
            Title="Championnats"
            ContentTemplate="{DataTemplate local:Championnat}"
            Route="Championnat" />
        </Tab>
    </TabBar> 
</Shell>
