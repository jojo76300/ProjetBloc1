<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="wwe.Pages.Championnat">
             <!--Title="Championnat"-->

    <VerticalStackLayout Padding="20" Spacing="10">
        <Label Text="{Binding Nom}" FontAttributes="Bold" FontSize="22"/>
        <Label Text="{Binding Info}" FontSize="16"/>
        <Label Text="{Binding DateCreation}" FontSize="16"/>
        <Label Text="{Binding Periode}" FontSize="16"/>
        <Label Text="{Binding TenantTitre}" FontSize="16"/>
    </VerticalStackLayout>

</ContentPage>
